{
    "name": "acc-extensions-vscode",
    "displayName": "Adobe Campaign Extensions",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jssp",
            "aliases": ["JSSP", "jssp"],
            "extensions": [".jssp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jssp",
            "scopeName": "source.jssp",
            "path": "./syntaxes/jssp.tmLanguage.json",
            "embeddedLanguages": {
                "source.js": "javascript",
                "text.html.derivative": "html",
                "meta.embedded.block.javascript": "javascript"
            }
        }]
    }
}